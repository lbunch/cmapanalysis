<cmap-measure>
	<Label>Correct Proposition Count</Label>
	<Description></Description>
	<DataType>Number</DataType>
	<Reference id="CorrectPropositions" description="An XML file containing a list of propositions each with a score"></Reference>
	<XQueryExpression>
	
	count(local:correct-propositions($map, doc("[[**Reference_CorrectPropositions**]]"))/cm:proposition)
	
	</XQueryExpression>
	<XQueryFunctions></XQueryFunctions>
</cmap-measure>